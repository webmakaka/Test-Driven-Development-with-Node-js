{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_PATH=./src nodemon --exec babel-core --config-file ./babel.config.json src/index.js",
    "babel": "babel-node --config-file ./babel.config.json src/index.js",
    "test": "jest --watchAll",
    "lint": "eslint . --fix",
    "build": "yarn clean && babel src --config-file ./babel.config.json -d dist",
    "prod:prepare": "NODE_PATH=./src yarn build && node --experimental-modules --es-module-specifier-resolution=node dist/index.js",
    "prod:run": "NODE_PATH=./src--experimental-modules --es-module-specifier-resolution=node dist/index.js",
    "clean": "rimraf dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "agent-base": "^6.0.2",
    "babel-plugin-transform-commonjs-es2015-modules": "^4.0.1",
    "express": "^4.17.1",
    "rimraf": "^3.0.2",
    "sequelize": "^6.6.5",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.9",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "eqeqeq": "warn",
      "prettier/prettier": "warn"
    }
  }
}
